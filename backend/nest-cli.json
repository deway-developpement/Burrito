{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "evaluations-ms": {
      "type": "application",
      "root": "apps/evaluations-ms",
      "entryFile": "main",
      "sourceRoot": "apps/evaluations-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/evaluations-ms/tsconfig.app.json"
      }
    },
    "forms-ms": {
      "type": "application",
      "root": "apps/forms-ms",
      "entryFile": "main",
      "sourceRoot": "apps/forms-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/forms-ms/tsconfig.app.json"
      }
    },
    "users-ms": {
      "type": "application",
      "root": "apps/users-ms",
      "entryFile": "main",
      "sourceRoot": "apps/users-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-ms/tsconfig.app.json"
      }
    },
    "analytics-ms": {
      "type": "application",
      "root": "apps/analytics-ms",
      "entryFile": "main",
      "sourceRoot": "apps/analytics-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics-ms/tsconfig.app.json"
      }
    },
    "groups-ms": {
      "type": "application",
      "root": "apps/groups-ms",
      "entryFile": "main",
      "sourceRoot": "apps/groups-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/groups-ms/tsconfig.app.json"
      }
    },
    "notifications-ms": {
      "type": "application",
      "root": "apps/notifications-ms",
      "entryFile": "main",
      "sourceRoot": "apps/notifications-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications-ms/tsconfig.app.json",
        "assets": [
          "notification/templates/**/*"
        ]
      }
    }
  }
}
